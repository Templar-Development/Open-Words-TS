{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
  "include": [
    "src/**/*.ts",
    "src/data/**/*.ts",
    "src/utils/**/*.ts",
    "src/utils/format/**/*.ts",
    "./search.ts",
    "./getData.ts"
  ],
  "exclude": ["node_modules"]
}
